
!------------------------------------------------------------------------------!
!*** Q24 ***!::: punch Q24b to Q24a for case 12549
!-----------!

if $iobs/12549, then,
        [*do i=1:[DataQ24.#]:2]
        [*do a=1:7]
                  if $Q24b_[DataQ24.i]/[a], then,
                          di $Q24a_[DataQ24.i]=[a], 
                          di $Q24b_[DataQ24.i]=0,
                  endif,             
        [*end a]
        [*end i]
endif,

!------------------------------------------------------------------------------!
!*** Q43 ***!::: Recode Q43 form code to att
!-----------!

!:1
[*data DataQ43C(;)=
1;	1;
2;	2;
3;	3;
4;	4;
99;	DK]

!:2
[*do i=1:[DataQ43.#]-1:2]
         dm $Q43_[DataQ43.i]=[*do x=1:99]f,[*end x]                    
[*end i]  

!:3
[*do i=1:[DataQ43.#]:2]
            [*do j=1:[DataQ43C.#]:2]
                        if $Q43C[DataQ43C.j+1]/[DataQ43.i], m $Q43_[DataQ43.i]/[DataQ43C.j],
            [*end j]
[*end i]

!------------------------------------------------------------------------------!
!*** Q43 ***!::: Recode Q43 combine Net Yes,
!-----------!

dm $Q43C_NET_Yes=$Q43C1.or.$Q43C2.or.$Q43C3,


!------------------------------------------------------------------------------!
!*** Q12a Net
!-----------!

!Voice 		= att 1
!Internet 	= att 9
!Word 		= att 11
!Personal email = att 7

[*data Q12aNet(;)=
Voice `+ Word;
Voice `+ Internet;
Voice `+ Internet `+ Word;
Voice `+ Internet `+ Personal Email;
Voice `+ Internet `+ Word `+ Personal Email]

	if ($Q12a_1/1..2).($Q12a_11/1..2), 					di $RQ12aNet_1=11,	
	if ($Q12a_1/1..2).($Q12a_9/1..2), 					di $RQ12aNet_2=11,	
	if ($Q12a_1/1..2).($Q12a_9/1..2).($Q12a_11/1..2), 			di $RQ12aNet_3=11,
	if ($Q12a_1/1..2).($Q12a_9/1..2).($Q12a_7/1..2), 			di $RQ12aNet_4=11,
	if ($Q12a_1/1..2).($Q12a_9/1..2).($Q12a_11/1..2).($Q12a_7/1..2),	di $RQ12aNet_5=11,
	
	
	[*do i=1:5]
	         if $RQ12aNet_[i]/n11, di $RQ12aNet_[i]=12,
	[*end i]

